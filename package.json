{
  "private": true,
  "main": "main/index.js",
  "productName": "Flowing",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "pnpm run build-electron && electron --enable-logging .",
    "dev:analyze": "ANALYZE=true pnpm run build-electron && electron .",
    "build-renderer": "next build renderer",
    "build-electron": "tsc -p electron-src",
    "build": "pnpm run build-renderer && pnpm run build-electron",
    "pack-app": "pnpm run build && electron-builder --dir",
    "dist": "pnpm run build && electron-builder",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "10.0.1",
    "@monaco-editor/react": "^4.5.1",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "clsx": "^2.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-next": "^3.1.5",
    "electron-store": "^8.1.0",
    "fast-fuzzy": "^1.12.0",
    "js-yaml": "^4.1.0",
    "monaco-editor": "^0.41.0",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-textarea-autosize": "^8.5.2",
    "reactflow": "^11.8.1",
    "swr": "^2.2.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.11",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.9",
    "autoprefixer": "^10.4.14",
    "electron": "^25.3.1",
    "electron-builder": "^24.4.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.11",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "next": "^13.4.13",
    "openapi-types": "^12.1.3",
    "postcss": "^8.4.26",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ],
    "extraResources": [
      "app/bin/"
    ]
  },
  "volta": {
    "node": "20.5.0"
  }
}